{
    "name": "just-luka/php-tron",
    "description": "PHP package for interacting with TRX network",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Trx\\": "src/Trx/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trx\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Luka T.",
            "email": "info.tsiklauri@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-gmp": "*",
        "kornrunner/secp256k1": "^0.3",
        "simplito/elliptic-php": "^1.0.12"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "monolog/monolog": "^3.0"
    }
}
